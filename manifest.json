{
    "manifest_version" : 2,

    "name"             : "example",
    "version"          : "0.0.0",
    "homepage_url"     : "http://example.com/extensions",
    "update_url"       : "http://example.com/extensions/example.xml",

    "description"      : "An example extension that exemplifies extensions.",

    "icons": {
        "16"  : "icons/icon16.png",
        "48"  : "icons/icon48.png",
        "128" : "icons/icon128.png"
    },

    "permissions": [
        "file:///*",
        "http://*/*",
        "https://*/*"
    ]
}
